    | sed -e "s: M ::g"\
    | sed -e "s: D ::g"\
    |grep "alps/") > /dev/null
    git status --porcelain

    ftEcho -y "请确认待commit修改列表,是否正确(回车默认y)"
    read -n 1 sel ;[ -z "${sel}" ] && sel=y
