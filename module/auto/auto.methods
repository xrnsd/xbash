    [[ -z $(which git) ]] && ftAutoCreateSxtdOldProjectBranch --rely "git" && return
    #解参验耦
    local valCount=1 errorContent
    local branchName=$(git branch --no-color 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/\1/')
    [ -z "$branchName" ] && errorContent="${errorContent}\\n[当前目录无有效git仓库]"
    [ -n "$errorContent" ] && ftEcho -ea "函数[${ftEffect}]的参数错误${errorContent}\\n请查看下面说明:" && ftAutoCreateSxtdOldProjectBranch -h && return

    #实现主体
    ftEcho -s "开始收集项目状态"
